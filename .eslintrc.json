{
    "env": {
        "node": true,
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "ignorePatterns": ["**/*.css", "**/*.scss"],
    "rules": {
        "no-mixed-spaces-and-tabs": "off",
        "react/prop-types": "off",
        "@typescript-eslint/no-unused-vars": 1,
        "@typescript-eslint/no-explicit-any": "error"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
// DO NOT DELETE FILE
