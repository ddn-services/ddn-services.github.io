{
    "name": "dashboard-systems",
    "version": "1.0.0",
    "title": "DevDaNang Admin Dashboard",
    "description": "This is the system dashboard using the services provided by DevDaNang systems",
    "author": {
        "name": "DevDaNang"
    },
    "private": false,
    "type": "module",
    "scripts": {
        "dev": "env-cmd -f .env.development vite",
        "build": "GENERATE_SOURCEMAP=false vite build",
        "preview": "vite preview",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "format": "prettier --write \"**/*.{js,jsx,json,md,html,css,less,scss}\"",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "prepare": "husky install",
        "tsc": "tsc"
    },
    "dependencies": {
        "@testing-library/jest-dom": "^6.1.6",
        "@testing-library/react": "^14.1.2",
        "i18next": "^23.7.13",
        "i18next-browser-languagedetector": "^7.2.0",
        "lint-staged": "^15.2.0",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-data-table-component": "^7.5.4",
        "react-dom": "^18.2.0",
        "react-i18next": "^14.0.0",
        "react-redux": "^9.0.4",
        "react-router-dom": "^6.21.1",
        "redux": "^5.0.1",
        "redux-saga": "^1.3.0",
        "vite-tsconfig-paths": "^4.2.0",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@vitejs/plugin-react": "^4.2.1",
        "axios": "^1.6.3",
        "axios-mock-adapter": "^1.22.0",
        "bootstrap": "^5.3.2",
        "classnames": "^2.5.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.2",
        "eslint-plugin-react": "^7.33.2",
        "firebase": "^10.7.1",
        "formik": "^2.4.5",
        "husky": "^8.0.3",
        "lodash": "^4.17.21",
        "metismenujs": "^1.4.0",
        "prettier": "3.1.1",
        "react-datepicker": "^4.25.0",
        "react-table": "^7.8.0",
        "reactstrap": "^9.2.1",
        "sass": "~1.64.2",
        "simplebar-react": "^3.2.4",
        "typescript": "^5.0.4",
        "vite": "^4.3.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "npx prettier --write .",
            "npx eslint --fix ."
        ],
        "*.scss": [
            "npx prettier --write ."
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "resolutions": {
        "moment": "2.29.1",
        "react-error-overlay": "6.0.11"
    }
}
